{
    "compilerOptions": {
        "baseUrl": "./src",
        "declaration": false,
        "downlevelIteration": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "lib": ["es2019", "dom"],
        "module": "commonjs",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noLib": false,
        "outDir": "./dist",
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "removeComments": true,
        "sourceMap": true,
        "strict": false,
        "target": "es5"
    }
}